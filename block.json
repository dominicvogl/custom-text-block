{
  "apiVersion": 2,
  "name": "dominic-vogl/text-box",
  "title": "Text Box",
  "description": "My second Block, a simple block that displays Text.",
  "category": "text",
  "keywords": [
	"text",
	"paragraph",
	"box"
  ],
  "textdomain": "textbox",
  "editorScript": "file:./build/index.js",
  "style": "file:./build/style-index.css",
  "editorStyle": "file:./build/index.css",
  "attributes": {
	"title": {
	  "type": "string",
	  "source": "html",
	  "selector": "h4"
	},
	"description": {
	  "type": "string",
	  "source": "html",
	  "selector": "p"
	},
	"alignment": {
	  "type": "string",
	  "default": "left"
	},
	"backgroundColor": {
	  "type": "string"
	},
	"textColor": {
	  "type": "string"
	}
  }
}
